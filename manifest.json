{
  "manifest_version": 3,
  "browser_specific_settings": {
    "gecko": {
      "id": "demophila@extensions.hask.moe",
      "strict_min_version": "120.0"
    }
  },
  "name": "demophila",
  "version": "1.0.2",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "Fill form fields with random data",
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"]
    }
  ]
}
